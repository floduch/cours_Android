<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_lock_foreground_redblue"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_lock_foreground_bluered"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginApp">
        <activity android:name=".DistrictActivity"></activity>
        <activity android:name=".QuizActivity" />
        <activity android:name=".ComplexActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>